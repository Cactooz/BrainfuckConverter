<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!--Color Variables-->
	<SolidColorBrush x:Key="Background" Color="#FFF8F8F8"/>
	<SolidColorBrush x:Key="Foreground" Color="#FF131313"/>
	<SolidColorBrush x:Key="Text" Color="#FF060606"/>

	<SolidColorBrush x:Key="TextboxBackground" Color="#FFF8F8F8"/>
	<SolidColorBrush x:Key="TextboxForeground" Color="#FF101010"/>

	<SolidColorBrush x:Key="ButtonBackground" Color="#FFB6B6B6"/>
	<SolidColorBrush x:Key="ButtonForeground" Color="#FF101010"/>
	<SolidColorBrush x:Key="ButtonDisabledBackground" Color="#FFEEEEEE"/>
	<SolidColorBrush x:Key="ButtonDisabledForeground" Color="#FFD0D0D0"/>
	<SolidColorBrush x:Key="ButtonPressedBackground" Color="#FFBFBFBF"/>

	<!--Icon Images-->
	<BitmapImage x:Key="AboutIcon" UriSource="../Images/Icons/Dark/about.png"/>
	<BitmapImage x:Key="ConvertIcon" UriSource="../Images/Icons/Dark/convert.png"/>
	<BitmapImage x:Key="CloseIcon" UriSource="../Images/Icons/Dark/close.png"/>
	<BitmapImage x:Key="FullscreenIcon" UriSource="../Images/Icons/Dark/fullscreen.png"/>
	<BitmapImage x:Key="MinimizeIcon" UriSource="../Images/Icons/Dark/minimize.png"/>
	<BitmapImage x:Key="SettingsIcon" UriSource="../Images/Icons/Dark/settings.png"/>
	<BitmapImage x:Key="WindowedIcon" UriSource="../Images/Icons/Dark/windowed.png"/>
	<BitmapImage x:Key="GithubIcon" UriSource="../Images/Icons/Dark/github.png"/>
</ResourceDictionary>
